{
  "name": "mimalyzer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run buildScalajsRelease && vite build",
    "preview": "vite preview",
    "dev": "concurrently npm:watchScalajs vite",
    "watchScalajs": "scala-cli package . -w -f -o scalajs-frontend.js --js-emit-source-maps",
    "buildScalajs": "scala-cli package . -f -o scalajs-frontend.js --js-emit-source-maps",
    "buildScalajsRelease": "scala-cli package . -f -o scalajs-frontend.js --js-mode release"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "^7.1.7"
  }
}
